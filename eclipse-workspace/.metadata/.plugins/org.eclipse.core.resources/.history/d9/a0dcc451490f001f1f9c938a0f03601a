package exception;

public class count {

	public static void main(String[] args) {
	 String s = "AABBAAC"; String a =s;
	 int count=1;
	 for(int i=0;i<s.length();i++) {
		 if(s.charAt(i)==a.charAt(i)) {
			count++;
			
		 }
		 System.out.println("s.charAt(i)" + count);
		 break;
		 
		 //System.out.println(i + count);
	 }
	 
	 

	}}
